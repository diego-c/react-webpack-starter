{
    "presets": [
        ["env", {
          "targets": {
              "browsers": [
                  "> 0.5%",
                  "last 2 versions"
                ]
            },
            "useBuiltIns": "entry"              
        }],
        "stage-2",
        "react"
    ],
    "env": {
        "test": {
            "plugins": ["transform-es2015-modules-commonjs","dynamic-import-node"]
        },
        "production": {
            "plugins": ["transform-es2015-modules-commonjs"]
        },
        "development": {
            "plugins": ["transform-es2015-modules-commonjs", "dynamic-import-node"]
        }
    }
}